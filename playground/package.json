{
  "private": true,
  "name": "playground",
  "scripts": {
    "start": "dumi dev",
    "build": "dumi build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\""
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@types/lodash.throttle": "^4.1.7",
    "@types/react-color": "^3.0.6",
    "dumi": "^1.0.13",
    "lint-staged": "^10.0.7",
    "lodash.throttle": "^4.1.1",
    "okey-icon-react": "workspace:0.0.1",
    "prettier": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "antd": "^4.21.6",
    "react-color": "^2.19.3"
  }
}
